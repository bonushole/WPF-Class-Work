<Window x:Class="BetterATM.WithdrawWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BetterATM"
        mc:Ignorable="d"
        Title="WithdrawWindow" Height="300" Width="300">
    <Grid>
      
        <Label x:Name="balanceLabel" />
        <TextBox x:Name="depositBox" HorizontalAlignment="Left" Height="23"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="87,69,0,0" />

        <Button x:Name="withdrawButton" Content="Withdraw" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="75" Click="withdrawButton_Click" Margin="106,117,0,0"/>
            <Button x:Name="returnButton" Content="Return" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click ="returnButton_Click" RenderTransformOrigin="0.5,0.5" Margin="106,161,0,0">
                
            </Button>
        
    </Grid>
</Window>
